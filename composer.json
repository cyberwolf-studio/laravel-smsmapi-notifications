{
    "name": "cyberwolfstudio/laravel-smsmapi-notifications",
    "description": "SmsAPI Notifications Channel for Laravel",
    "homepage": "https://github.com/cyberwolf-studio/laravel-smsmapi-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyberwolf.Studio",
            "email": "hello@cyberwolf.studio",
            "homepage": "https://cyberwolf.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0 || ^11.0 || ^12.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0 || ^11.0 || ^12.0",
        "smsapi/php-client": "^1.8 || ^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "CyberWolfStudio\\LaravelSmsAPINotifications\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CyberWolfStudio\\LaravelSmsAPINotifications\\SmsAPIServiceProvider"
            ]
        }
    }
}
